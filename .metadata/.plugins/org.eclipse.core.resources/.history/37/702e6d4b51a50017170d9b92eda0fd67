package election.business;

public class DawsonItemBallot {
	
	
	private String choiceString;
	private int value;
	private int final maxValue;
	
	
	public DawsonItemBallot(String choiceString, int value){
		
		this.choiceString=choiceString;
		this.value=0;
	}
	
	
	public DawsonItemBallot(int maxValue){
		
		this.maxValue=maxValue;
		
	}

}
